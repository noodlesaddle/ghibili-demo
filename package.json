    "axios": "^1.4.0",
    "dayjs": "^1.11.8",
    "framer-motion": "^10.12.17",
    "i18next": "^23.2.3",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
